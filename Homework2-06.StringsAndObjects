using System;

namespace StringsAndObjects
{
    class Program
    {
        static void Main(string[] args)
        {

            string hello = "Hello";
            string world = "World";

            object helloWorld = (hello + " " + world);

            string objectValue = (string) helloWorld;

            {
                Console.WriteLine(objectValue);
            }

        }
    }
}

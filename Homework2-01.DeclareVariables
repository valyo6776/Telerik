using System;

namespace DeclareVariables
{
    class Program
    {
        static void Main()
        {
            ushort a = 52130;
            sbyte b = -115;
            int c = 4825932;
            byte d = 97;
            short e = -10000;

            Console.WriteLine(a + "\n" + b+ "\n" + c + "\n" + d+ "\n" + e);
                      
        }
    }
}

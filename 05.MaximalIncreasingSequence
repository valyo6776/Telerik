using System;

namespace _05.MaximalIncreasingSequence
{
    class Program
    {
        static void Main(string[] args)
        {
            //input length
            int n = int.Parse(Console.ReadLine());

            //initialize array
            int[] arr = new int[n];

            //input first number
            int firstNum = int.Parse(Console.ReadLine());

            arr[0] = firstNum;

            //initialize variables
            int maxSequence = 0;
            int tempSequence = 1;

            //input next n-1 numbers + calculations
            for (int i = 1; i < n; i++)
            {
                arr[i] = int.Parse(Console.ReadLine());

                if (arr[i] > arr[i-1])
                {
                    tempSequence++;
                    maxSequence = Math.Max(tempSequence, maxSequence);
                }
                else
                {
                    tempSequence = 1;
                }
            }

            //print result
            Console.WriteLine(maxSequence);
        }
    }
}
